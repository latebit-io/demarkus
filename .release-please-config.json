{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "separate-pull-requests": true,
  "packages": {
    "protocol": {
      "release-type": "go",
      "package-name": "protocol",
      "include-v-in-tag": true,
      "tag-separator": "/",
      "changelog-path": "CHANGELOG.md"
    },
    "server": {
      "release-type": "go",
      "package-name": "server",
      "include-v-in-tag": true,
      "tag-separator": "/",
      "changelog-path": "CHANGELOG.md"
    },
    "client": {
      "release-type": "go",
      "package-name": "client",
      "include-v-in-tag": true,
      "tag-separator": "/",
      "changelog-path": "CHANGELOG.md"
    }
  }
}
